PREFIX :       <https://github.com/wintechis/RDeF#>
PREFIX dct:    <http://purl.org/dc/terms/>
PREFIX foaf:   <http://xmlns.com/foaf/spec/#> 
PREFIX rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT ?author
WHERE {
    _:b0    rdf:type        :Story  ;
            dct:creator      _:b1 .
    _:b1    foaf:name       ?author .
}
         