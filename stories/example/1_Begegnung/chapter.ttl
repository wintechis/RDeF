PREFIX :       <https://www.example.org/>  
PREFIX schema: <https://schema.org/> 
PREFIX dct:    <http://purl.org/dc/terms/>
PREFIX rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#>   
PREFIX rdfs:   <https://www.w3.org/2000/01/rdf-schema#>



:chapter1   rdf:type        :Chapter    ;
            rdfs:label      'Begegnung' ;
            :hasEpisodes      _:b0        .


_:b0        rdf:type        rdf:Bag  ;
            rdf:_1          :ep1     ;
            rdf:_2          :ep2     .



:ep1        rdf:type            :Episode    ;
            rdfs:label          'Titel 1'   ;
            schema:location     :berlin     ;
            dct:abstract        'das ist die Beschreibung 1' ;
            schema:attendee     :jose   .
           

:ep2     rdf:type               :Episode    ;
            rdfs:label          'Titel 2'   ;
            schema:location     :sydney  ;
            dct:abstract        'das ist die Beschreibung 2' ;
            schema:attendee     :jack   .

